{
    "name": "zalas/toolbox",
    "description": "Helps to discover and install tools",
    "type": "project",
    "require": {
        "php": "^7.2",
        "symfony/console": "^4.4 || ^5.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.3",
        "zalas/phpunit-globals": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Toolbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\Toolbox\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}

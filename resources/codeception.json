    {
      "name": "codeception",
      "summary": "Codeception is a BDD-styled PHP testing framework",
      "website": "https://codeception.com/",
      "command": {
        "composer-bin-plugin": {
          "package": "codeception/codeception",
          "namespace": "codeception",
          "links": {"%target-dir%/codecept": "codecept"}
        }
      },
      "test": "codecept --version",
      "tags": ["test", "codeception"]
    },
